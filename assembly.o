.section .text
.global _start
_start:

mov r0,#1
mov r1,#2
	
cmp r0,r1

bgt greater
blt lesser
	
	
	
greater:	
	mov r0,#0
	ldr r1,=msg
	mov r2,#12
	mov r7,#4
	svc #0
	
	mov r0,#0
	mov r7,#1
	svc #0
	
lesser:
	mov r0,#0
	ldr r1,=msg2
	mov r2,#11
	mov r7,#4
	svc #0
	
	mov r0,#0
	mov r7,#1
	svc #0
	
	

.section .data	
msg: .ascii "hello world\n"
msg2: .ascii "hello Dave\n"
	
